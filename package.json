{
  "name": "fastify",
  "type": "module",
  "main": "api/index.js",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "echo 'Setting up environment...' && export ONNX_DISABLE_BACKEND=1 && export SKIP_ONNXRUNTIME_POSTINSTALL=1",
    "debug": "vercel dev",
    "build": "tsc && node vercel-build.js && node scripts/verify-build.js",
    "dev": "NODE_ENV=development tsx --watch api/index.ts",
    "start": "node .vercel/output/functions/api/index.js",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "vercel-build": "tsc && node vercel-build.js",
    "verify-build": "node scripts/verify-build.js"
  },
  "dependencies": {
    "@coinbase/agentkit": "^0.7.2",
    "@coinbase/cdp-sdk": "^1.10.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/vite": "^8.1.3",
    "@gradio/client": "^1.14.2",
    "@mastra/client-js": "^0.1.22",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.1",
    "fastify": "^5.3.3",
    "openai": "^4.100.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.47",
    "@vercel/node": "^3.0.12",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
